  <script>
    $(function () {
      var socket = io();

      $('form').submit(function () {
        socket.emit('chat message', $('#m').val());
        $('#m').val('');
        return false;
      });

      socket.on('recibido', function (objeto) {
        $('#messages').append($('<li>').text(objeto.txt + ' - ' + objeto.date));
        window.scrollTo(0, document.body.scrollHeight);
      });
    });

  </script>
